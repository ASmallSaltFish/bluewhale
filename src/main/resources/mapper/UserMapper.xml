<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qs.bluewhale.mapper.UserMapper">
    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        user_id,
        user_name,
        nick_name,
        email,
        phone,
        password,
        position,
        sex,
        signature,
        user_status,
        create_time,
        create_by,
        last_modify_time,
        last_modify_by
    </sql>

    <!--插入sql语句-->
    <insert id="saveUser" parameterType="com.qs.bluewhale.entity.User">
        insert into t_bw_user(user_id,user_name,password,sex,signature)
        values (#{userId},#{userName},#{password},#{sex},#{signature})
    </insert>

</mapper>